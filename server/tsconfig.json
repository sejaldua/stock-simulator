{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "baseUrl": "./",
      "traceResolution": false,
      "rootDirs": [
        "src"
      ],
      "paths": {
      },
      "checkJs": false,
      "allowUnreachableCode": true,
      "resolveJsonModule": true,
      "jsx": "react",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "declaration": false,
      "noImplicitAny": false,
      "noImplicitReturns": false,
      "noUnusedLocals": false,
      "removeComments": true,
      "strictNullChecks": false,
      "allowJs": true,
      "moduleResolution": "node",
      "outDir": "dist",
      "lib": [
        "es6",
        "dom",
        "es7"
      ],
      "sourceMap": true
    },
    "lib": [
      "dom",
      "es6",
      "dom",
      "es2016"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "node_modules",
      "nswag"
    ]
  }